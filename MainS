function findflower()
    local flowerrrr = flowertable[math.random(#flowertable)]
    if tonumber((flowerrrr.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude) <= 50 and tonumber((flowerrrr-fieldposition).magnitude) <= 50 then 
        if temptable.running == false then
            plr.Character.Humanoid:MoveTo(flowerrrr)
        end 
    end
end

spawn(function()
	while wait() do
		if bounty.toggles.autofarm then
			if game.Players.LocalPlayer.Character:FindFirstChild("ProgressLabel",true) then
	        local pollenprglbl = game.Players.LocalPlayer.Character:FindFirstChild("ProgressLabel",true)
	        local maxpollen = tonumber(pollenprglbl.Text:match("%d+$"))
	        local pollencount = game.Players.LocalPlayer.CoreStats.Pollen.Value
	        local pollenpercentage = pollencount/maxpollen*100
	        local fieldselected = game:GetService("Workspace").FlowerZones[bounty.field]
	        local fieldpos = CFrame.new(fieldselected.Position.X, fieldselected.Position.Y+3, fieldselected.Position.Z)
       		local fieldposition = fieldselected.Position
       		if tonumber(pollenpercentage) < tonumber(bounty.convertat) then
       			if not bounty.tembs.tokensfarm then
       				tweens(1, fieldpos)
       				bounty.tembs.tokensfarm = true
       			else
       				if (fieldposition-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude > temptable.magnitude then
                   		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = fieldpos.CFrame
                    	--if bounty.toggles.autosprinkler then makesprinklers() end
               		end
               		--if bounty.toggles.avoidmobs then avoidmob() end
	                --if bounty.toggles.farmclosestleaf then closestleaf() end
	                --if bounty.toggles.farmbubbles then getbubble() end
	                --if bounty.toggles.farmclouds then getcloud() end
       			end
       		elseif tonumber(pollenpercentage) >= tonumber(bounty.convertat) then
       			bounty.tembs.tokensfarm = false
       			tweens(1, plr.SpawnPos.Value * CFrame.fromEulerAnglesXYZ(0, 110, 0) + Vector3.new(0, 0, 9))
       			bounty.tembs.converting = true
       		end
		end
	end
end)

spawn(function()
	while wait(1) do
		bounty.playtime += 1
		bounty.honeycurrent = statsget().Totals.Honey
		GainedHoney.Text = "Gained Honey: "..v9(bounty.honeycurrent-bounty.tembs.honeystart)
		GainedSnowflakes.Text = "Farmed Snowflakes: "..v9(rtsg().Eggs.Snowflake-bounty.tembs.Snowflakesstart)
		Playtime.Text = "Time in game: "..FormatTime(bounty.playtime)
	end
end)

spawn(function()
	while wait(0.05) do
		plr.Character.Humanoid.WalkSpeed = bounty.choosedspeed
		if bounty.toggles.AutoDig == true then
			game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool"):FindFirstChild("ClickEvent", true):FireServer()
		end
	end
end)

spawn(function()
	while true do
		ScriptLabel.Text = "  Bounty | Alpha 0.0.1  "
		wait(0.15)
		ScriptLabel.Text = " • Bounty | #lpha 0.0.1 • "
		wait(0.175)
		ScriptLabel.Text = " • • Bounty | A$pha 0.0.1 • • "
		wait(0.2)
		ScriptLabel.Text = " • • • Bounty | Al&ha 0.0.1 • • • "
		wait(0.25)
		ScriptLabel.Text = "  Bounty | Alp~a 0.0.1  "
		wait(0.15)
		ScriptLabel.Text = " • Bounty | Alph? 0.0.1 • "
		wait(0.175)
		ScriptLabel.Text = " • • Bounty | #$&~? 0.0.1 • • "
		wait(0.2)
		ScriptLabel.Text = " • • • Bounty | ?~&$# 0.0.1 • • • "
		wait(0.25)
	end
end)
