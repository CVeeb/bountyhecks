function ChooseFrame()
	local frames = {
		["HomeF"] = true,
		["FarmF"] = true,
		["CombatF"] = true,
		["HiveF"] = true,
		["SettingsF"] = true,
		["TimersF"] = true,
	}
	for i,v in pairs(frame:GetChildren()) do
		if frames[v.Name] then
			if v.Name ~= OMen then
				v.Visible = false
			end
		end
	end
end

ShowUI.MouseButton1Click:Connect(function()
	--showed = true
	--frame:TweenPosition(UDim2.new(0,frame.Position.X.Offset,0,frame.Position.Y.Offset -2000), "Out", "Back", 0.5, true)
	if ShowUI.Text == "-" then
		ShowUI.Text = "+"
		hidedui = true
		frame:TweenSize(UDim2.new(0,Xsize,0,33), "Out", "Back", 0.25, true)
		OMen.Visible = false
		Menus.Visible = false
	else
		ShowUI.Text = "-"
		hidedui = false
		frame:TweenSize(UDim2.new(0,Xsize,0,YSize), "Out", "Back", 0.25, true)
		OMen.Visible = true
		Menus.Visible = true
	end
end)
