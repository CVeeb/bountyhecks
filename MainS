function statsget()
	local StatCache = require(game.ReplicatedStorage.ClientStatCache)
	local stats = StatCache:Get()
	return stats
end
for k,g in pairs(workspace.FieldDecos:GetChildren()) do
	if g:IsA("BasePart") or g:IsA("MeshPart") or g:IsA("Part") then
		g.Transparency = 0.65
		g.CanCollide = false
	elseif g:IsA("Model") then
		for i,v in pairs(g:GetChildren()) do
			if v:IsA("BasePart") or v:IsA("MeshPart") or v:IsA("Part") then
				v.Transparency = 0.65
				v.CanCollide = false
			elseif g:IsA("Model") then
				for _,x in pairs(g:GetChildren()) do
					if x:IsA("BasePart") or x:IsA("MeshPart") or x:IsA("Part") then
						x.Transparency = 0.65
						x.CanCollide = false
					end
				end
			end
		end
	end
end

